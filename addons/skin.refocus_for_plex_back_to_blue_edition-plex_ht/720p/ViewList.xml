	<include name="view_List">
		<control type="list" id="50">
			<viewtype label="535">list</viewtype> 	
			<posy>90</posy>
			<width>625</width>
			<height>612</height>
			<onup>50</onup>
			<ondown>50</ondown>
			<onleft>300</onleft>
			<onright>80</onright>
			<pagecontrol>80</pagecontrol>
			<scrolltime tween="Cubic" easing="Out">200</scrolltime>
			<preloaditems>2</preloaditems>
			<orientation>vertical</orientation>
			<include>transition_Container</include>
			<include>infoDialogHide</include>
			<include>contextMenuHide</include>
			<include condition="Window.IsVisible(videolibrary)">List_Video</include>
			<include condition="Window.IsVisible(videofiles)">List_Video</include>
			<include condition="Window.IsVisible(musicfiles) | Window.IsActive(502)">List_Music</include>
			<include condition="Window.IsVisible(videoplaylist)">List_VideoPlaylist</include>
			<include condition="Window.IsVisible(musicplaylist)">List_MusicPlaylist</include>
			<include condition="Window.IsVisible(pictures) | Window.IsVisible(programs) | Window.IsVisible(addonbrowser)">List_Files</include>
			<include condition="Window.IsVisible(sharedcontent)">List_Shared</include>
		</control>
	</include>
		
